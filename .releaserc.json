{
  "branches": [
    {
      "name": "master"
    },
    {
      "name": "develop",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "mv dev/info.json . && python3 -u dev/package.py \"${nextRelease.version}\""
      }
    ],
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failComment": false,
        "assets": [{ "path": "*.zip" }, { "path": "manifest.json" }]
      }
    ]
  ]
}
